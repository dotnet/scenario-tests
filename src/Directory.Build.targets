<Project>

  <Import Project="Sdk.targets" Sdk="Microsoft.DotNet.Arcade.Sdk" />

  <!-- Override the test target to run the test harness as a separate executable against dotnet SDK used to run these tests. -->
  <Target Name="Test">
    <Exec Command="$(DotNetTool) $(TargetPath) --dotnet-root $(DotNetRoot) --sdk-version $(NETCoreSdkVersion)" />
  </Target>
</Project>
